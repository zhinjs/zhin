        op: OpCode;
        d: any;
    };
    constructor(bot: QQBot);
    getAccessToken(): Promise<QQBot.Token>;
    getWsUrl(): Promise<void>;
    getValidIntends(): number;
    start(): Promise<void>;
    stop(): Promise<void>;
    private checkNeedToRestart;
    connect(): void;
    reconnectWs(): void;
