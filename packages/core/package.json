{
  "name": "@zhin.js/core",
  "version": "1.0.17",
  "description": "Zhin机器人核心框架",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "development": "./src/index.ts",
      "import": "./lib/index.js"
    },
    "./setup": {
      "types": "./lib/setup.d.ts",
      "development": "./src/setup.ts",
      "import": "./lib/setup.js"
    },
    "./jsx": {
      "types": "./lib/jsx.d.ts",
      "development": "./src/jsx.ts",
      "import": "./lib/jsx.js"
