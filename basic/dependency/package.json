{
  "name": "@zhin.js/dependency",
  "version": "1.0.3",
  "description": "A TypeScript dependency tree analyzer with ESM support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register.mjs": "./loaders/register.mjs",
    "./tsx-loader.mjs": "./loaders/tsx-loader.mjs",
    "./bun-plugin": "./loaders/bun-plugin.ts",
    "./bun-preload.ts": "./loaders/bun-preload.ts"
  },
  "files": [
    "dist",
    "loaders",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dependency",
    "tree",
    "module",
    "import",
    "loader",
    "typescript",
    "esm"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/bun": "^1.2.0",
    "typescript": "^5.3.0"
  }
}
