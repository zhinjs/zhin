{
  "name": "@zhin.js/dependency",
  "version": "1.0.5",
  "description": "TypeScript dependency tree analyzer with ESM support for Zhin.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register.mjs": "./loaders/register.mjs",
    "./tsx-loader.mjs": "./loaders/tsx-loader.mjs",
    "./bun-plugin": "./loaders/bun-plugin.ts",
    "./bun-preload.ts": "./loaders/bun-preload.ts"
  },
  "files": [
    "dist",
    "loaders",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "zhin",
    "bot",
    "dependency",
    "tree",
    "module",
    "import",
    "typescript",
    "esm"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/bun": "^1.2.0",
    "typescript": "^5.3.0",
    "zhin.js": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhinjs/zhin.git",
    "directory": "basic/dependency"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "zhin.js": "workspace:*"
  }
}
